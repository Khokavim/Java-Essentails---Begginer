package Tutorials;

import java.lang.reflect.Array;

public class Exceptions {

    private final int[] array= {};
    
    public Exceptions(int k) {
    	for(int i=k; i < Array.getLength(array);i--) {
    		array[i]=k;
    		System.out.println("K value" + k);
    		System.out.println("K value" + k);
    	}
    }
}
